{
  "name": "vscode-newman-tcp-proxy",
  "displayName": "Newman TCP Proxy",
  "description": "VS Code extension for running Newman tests through an integrated TCP proxy",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-newman-tcp-proxy.start",
        "title": "Start Newman TCP Proxy"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile-webview && npm run compile",
    "compile": "tsc -p ./",
    "compile-webview": "webpack --mode development",
    "watch": "tsc -watch -p ./",
    "watch-webview": "webpack --mode development --watch",
    "pretest": "npm run compile-webview && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.1.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
